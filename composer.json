{
  "name": "nadiyas/magento2ce-patches",
  "description": "Magento 2 patches",
  "type":"magento2-patches",
  "version": "2.2.3",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "repositories": {
    "path": {
      "type": "git",
      "url": "git@github.com:nadiyas/magento2-patches-tool.git"
    }
  },
  "require": {
    "nadiyas/magento2-patches-tool": "dev-master"
  },
  "minimum-stability": "dev",
  "extra": {
    "enable-patching": true,
    "patch-dir": "patches",
    "patches":{
      "1": {
        "name": "patch1"
      },
      "2": {
        "name": "patch2",
        "after": 1
      }
    }
  }
}
